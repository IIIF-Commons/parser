{
  "name": "@iiif/parser",
  "version": "1.0.14",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "./.build/types/index.d.ts",
  "files": [
    ".build",
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/presentation-3/cjs/index.js",
      "import": "./dist/presentation-3/index.mjs",
      "default": "./dist/presentation3/index.umd.js"
    },
    "./presentation-2": {
      "require": "./dist/presentation-2/cjs/index.js",
      "import": "./dist/presentation-2/esm/index.mjs",
      "default": "./dist/presentation-2/index.umd.js"
    },
    "./presentation-3": {
      "require": "./dist/presentation-3/cjs/index.js",
      "import": "./dist/presentation-3/esm/index.mjs",
      "default": "./dist/presentation-3/index.umd.js"
    },
    "./upgrader": {
      "require": "./dist/upgrader/cjs/index.js",
      "import": "./dist/upgrader/esm/index.mjs",
      "default": "./dist/upgrader/index.umd.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        ".build/types/index.d.ts"
      ],
      "presentation-2": [
        ".build/types/presentation-2/index.d.ts"
      ],
      "presentation-3": [
        ".build/types/index.d.ts"
      ],
      "upgrader": [
        ".build/types/upgrader.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p . --declaration --emitDeclarationOnly && node ./vite/build.mjs",
    "prepublishOnly": "tsc -p . --declaration --emitDeclarationOnly && node .vite/build.mjs",
    "test": "vitest"
  },
  "dependencies": {
    "@iiif/presentation-2": "^1.0.3",
    "@iiif/presentation-3": "^1.1.3",
    "@types/geojson": "^7946.0.8"
  },
  "devDependencies": {
    "@hyperion-framework/validator": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chalk": "^5.0.1",
    "eslint": "^8.7.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
